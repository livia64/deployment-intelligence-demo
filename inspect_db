import duckdb

con = duckdb.connect("deployment.db")

tables = con.execute("SHOW TABLES").fetchall()
print("Tables found:")
for t in tables:
    print("-", t[0])

print("\nRow counts:")
for t in tables:
    name = t[0]
    count = con.execute(f"SELECT COUNT(*) FROM {name}").fetchone()[0]
    print(f"- {name}: {count}")

con.close()

